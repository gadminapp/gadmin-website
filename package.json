{
  "name": "gadmin-landing",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --remote",
    "build": "astro check && astro build",
    "astro": "astro",
    "check": "astro check && tsc --noEmit",
    "preview": "wrangler pages dev dist",
    "deploy": "wrangler pages deploy dist",
    "lint": "eslint --ext=.astro,.ts,.js,.tsx .",
    "lint:fix": "eslint --ext=.astro,.ts,.js,.tsx . --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/cloudflare": "^10.2.2",
    "@astrojs/sitemap": "^3.1.4",
    "aos": "^2.3.4",
    "astro": "^4.6.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.7.1",
    "astro-eslint-parser": "^1.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.1.0",
    "husky": "^9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-astro-organize-imports": "^0.4.5",
    "wrangler": "^3.51.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx)": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  }
}
